@page "/BaseServices"
@inherits Ace.AceGUI.Pages.BaseServicesCodeBehind
<h1>AceCommander Base Services</h1>

@if (@BaseServicesInitializationRspPayload == null)
{
  <p>Loading (Calling onInitAsync)</p>
}
else
{
  @if (@IsAliveRspPayload == null)
  {
    <p>Getting IsAlive..</p>
  }
  else
  {
    <p>IsAliveRspPayload = @IsAliveRspPayload</p>
  }
  <p>BaseServicesInitializationRspPayload = @BaseServicesInitializationRspPayload</p>
}

